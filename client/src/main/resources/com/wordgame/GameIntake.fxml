<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane
  maxHeight="-Infinity"
  maxWidth="-Infinity"
  minHeight="-Infinity"
  minWidth="-Infinity"
  prefHeight="400.0"
  prefWidth="600.0"
  xmlns="http://javafx.com/javafx/23"
  xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="com.wordgame.GameIntakeController"
>
   <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
      <VBox alignment="TOP_CENTER" prefHeight="278.0" prefWidth="435.0">
         <Label text="Diction-ary!">
            <font>
               <Font name="Curlz MT" size="34.0" />
            </font>
            <VBox.margin>
               <Insets bottom="50.0" />
            </VBox.margin>
         </Label>
         <Button fx:id="easyButton" mnemonicParsing="false" text="Easy">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin>
         </Button>
         <Button fx:id="mediumButton" mnemonicParsing="false" text="Medium">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin>
         </Button>
         <Button fx:id="hardButton" mnemonicParsing="false" text="Hard"/>
         <HBox.margin>
            <Insets bottom="10.0" />
         </HBox.margin>
      </VBox>
   </HBox>
</StackPane>
